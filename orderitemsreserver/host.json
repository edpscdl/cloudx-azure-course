{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true
      }
    },
    "logLevel": {
      "default": "Information"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "healthMonitor": {
    "enabled": true
  },
  "retry": {
    "strategy": "exponentialBackoff",
    "minimumInterval": "00:00:30",
    "maximumInterval": "00:01:00",
    "maxRetryCount": 3
  }
}